  "devDependencies": {
    "eslint": "^3.12.2",
    "jenkins-mocha": "^4.1.0",
    "nock": "^9.0.2",
    "nyc": "^10.0.0"
  },
  "dependencies": {
    "debug": "^2.6.8",
    "socket.io": "^2.1.1"
